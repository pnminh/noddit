{
   "name": "noddit",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "copy-static": "ncp src build --filter='^[^.]+$|\\.(?!(ts)$)([^.]+$)'",
      "build": "./node_modules/typescript/bin/tsc&&npm run copy-static",
      "build-webpack": "webpack --config webpack.config.js",
      "postinstall": "npm run build",
      "test": "export NODE_ENV=test&&jasmine-ts",
      "start": "ts-node src/index.ts",
      "start-watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/StartServer.ts",
      "start-local": "ts-node src/StartServer.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/dotenv": "^6.1.0",
      "@types/ejs": "^2.6.0",
      "@types/express": "^4.16.0",
      "@types/jasmine": "^3.3.0",
      "@types/node": "^8.0.29",
      "@types/request": "^2.48.1",
      "@types/sequelize": "^4.27.31",
      "copy-webpack-plugin": "^4.6.0",
      "cpx": "^1.5.0",
      "jasmine": "^3.3.0",
      "jasmine-ts": "^0.3.0",
      "ncp": "^2.0.0",
      "nodemon": "^1.18.6",
      "request": "^2.88.0",
      "ts-loader": "^5.3.0",
      "ts-node": "3.3.0",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6",
      "webpack": "^4.26.0",
      "webpack-cli": "^3.1.2"
   },
   "dependencies": {
      "dotenv": "^6.1.0",
